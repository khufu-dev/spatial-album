{
  "version": "0.0.1",
  "tasks": {
    "dev": "deno run -A npm:vite",
    "dev:avp": "XR_ENV=avp deno run -A npm:vite",
    "build": "deno run -A npm:vite build && XR_ENV=avp deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "avp": "deno run -A npm:@webspatial/builder/webspatial-builder run --base=http://localhost:5173/webspatial/avp",
    "build:avp": "deno run -A npm:@webspatial/builder/webspatial-builder build --base= --bundle-id=com.picoxr.spatialalbum --teamId=KQNC4RNTJN",
    "publish": "deno run -A npm:@webspatial/builder/webspatial-builder publish --base= --bundle-id=com.picoxr.spatialalbum --teamId=KQNC4RNTJN --version=$XR_VERSION --u=$XR_DEV_NAME --p=$XR_DEV_PASSWORD"
  },
  "imports": {
    "@google/model-viewer": "npm:@google/model-viewer",
    "@webspatial/builder": "npm:@webspatial/builder",
    "@webspatial/core-sdk": "npm:@webspatial/core-sdk",
    "@webspatial/platform-visionos": "npm:@webspatial/platform-visionos",
    "@webspatial/react-sdk": "npm:@webspatial/react-sdk",
    "@webspatial/vite-plugin": "npm:@webspatial/vite-plugin",
    "react": "npm:react",
    "react-dom": "npm:react-dom",
    "three": "npm:three",
    "@types/react": "npm:@types/react",
    "vite-plugin-html": "npm:vite-plugin-html"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "deno.ns"],
    "types": ["react", "react-dom", "@types/react", "@types/node"],
    "jsx": "react-jsx",
    "jsxImportSource": "@webspatial/react-sdk"
  },
  "nodeModulesDir": "auto"
}
