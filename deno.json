{
  "version": "0.0.1",
  "tasks": {
    "dev": "deno run -A npm:vite",
    "dev:avp": "XR_ENV=avp deno run -A npm:vite",
    "build": "deno run -A npm:vite build && XR_ENV=avp deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "avp": "deno run -A npm:@webspatial/builder/webspatial-builder run --base=http://localhost:5173/webspatial/avp"
  },
  "imports": {
    "@webspatial/platform-visionos": "npm:@webspatial/platform-visionos",
    "@webspatial/react-sdk": "npm:@webspatial/react-sdk",
    "react": "npm:react",
    "react-dom": "npm:react-dom",
    "@types/react": "npm:@types/react"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "deno.ns"],
    "types": ["react", "react-dom", "@types/react", "@types/node"],
    "jsx": "react-jsx",
    "jsxImportSource": "@webspatial/react-sdk"
  },
  "nodeModulesDir": "auto"
}
